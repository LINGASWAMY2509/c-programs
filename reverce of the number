#include <stdio.h>

int main()
{
   int i,j ,temp=0,a[10]={1,4,7,2,8,15,16,17,20,13};
   
   for(j=0;j<10;j++)
   {
    for(i=0;i<10;i++)
     {
      if(a[i]>a[i+1])
      {
          temp=a[i];
          a[i]=a[i+1];
          a[i+1]=temp;
      }
     }
   }
   printf("assending order of elements are\n\n");
       for(i=0;i<10;i++)
    {
        printf("%d\t",a[i]);
    }
}
