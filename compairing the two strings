#include<stdio.h>
#include<string.h>
void main()
{ 
    int i;
    char a[10]={"ixilinks"},n,n1,g,f;
    char b[10]={"abinks"};
    char c[10],d[10],e[10];
    n=strlen(a);
    for(int i=0;0<n;i++)
    {
        c[i]=a[n-1];
        n--;
    }
    n1=strlen(b);
    for(int i=0;0<n1;i++)
    {
        d[i]=b[n1-1];
        n1--;
    }
    g=strlen(d);
    printf("after compairing the two strings result is\n");
    for(int i=0;0<g;i++)
    f=g;
    {
      if(c[i]==d[i])
      {
        printf("%c\t",c[i]);
        e[i]=c[f-1];
      }
      else
      {
        printf("there is no equal charecter\n");
      }
    }
    for(int i=0;0<f;i++)
    {
        printf("%c\t",e[i]);
        f--;
    }
    g--;
}
